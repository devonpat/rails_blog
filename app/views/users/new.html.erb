<h1>Users#new</h1>
<p>Find me in app/views/users/new.html.erb</p>


<div id="new_user">
	<%= form_with model: @user, local: true do |f| %>
		<%= f.label :first_name %>
		<%= f.text_field :first_name %>
		<br>
		<%= f.label :last_name %>
		<%= f.text_field :last_name %>
		<br>
		<%= f.label :email %>
		<%= f.text_field :email%>
		<br>
		<%= f.label :password %>
		<%= f.text_field :password %>
		<br>
		<%= f.check_box(:chickens) %>
		<%= label_tag(:chickens_yes, "I have chickens") %>
		<br>
		<%= f.check_box(:bees) %>
		<%= label_tag(:bees_yes, "I have Bees") %>
		<br>
		<%= f.check_box(:bats) %>
		<%= label_tag(:bats_yes, "I have Bats") %>
		<br>
		<%= f.check_box(:dogs) %>
		<%= label_tag(:dogs_yes, "I have Dogs") %>
		<br>
		<%= f.check_box(:cats) %>
		<%= label_tag(:cats_yes, "I have Cats") %>
		<br>
		<%= f.label :city_or_Farm? %>
		<%= f.text_field :home_type %>
		<%= f.submit %>
	<% end %>
</div>

